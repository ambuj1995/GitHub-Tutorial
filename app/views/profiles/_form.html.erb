  <%= form.radio_button(:gender, "female") %>
  <%= form.label(:gender_female, "Female") %>
  <br/>
  <br/>
  <%= form.label :Mobile %>
  <%= form.telephone_field :contact%>
  <br/>
  <br/>
  <%= form.label :City %>
  <%= form.text_field :city%>
  <br/>
  <br/>
  <%= form.label :State %>
  <%= form.text_field :state%>
  <br/>
  <br/>
  <%= form.label :Country %>
  <%= form.text_field :country%>
  <br/>
  <br/>
  <%= form.label :Zip_code %>
  <%= form.text_field :zipcode%>
  <br/>
  Image Upload:
  <%= form.file_field :avatar %>
  <br><br>
  
 
  <%= form.submit %>
<% end %>